Example Code:
----------------------------------------------------------------------------------------
unsigned a b c
signed x y z

a = 3
b = 15 + a
c = b * a / 10

x = -5
y = 13

if c <= 10
	x = y + 10
else
	x = y - 20

while y > 0
	print y
	print \n
	print x
	print \n
	y = y - 1

----------------------------------------------------------------------------------------
Conversions from Assembly to Machine Code:
----------------------------------------------------------------------------------------


                Assembly                |   		Machine Code    
----------------------------------------------------------------------------------------
mov ecx, eax                            |   00 XX XX               						
----------------------------------------------------------------------------------------
add ecx, eax, ebx                       |   0A XX XX XX           						
----------------------------------------------------------------------------------------
sub ecx, eax, ebx                       |   0B XX XX XX               					
----------------------------------------------------------------------------------------
mult ecx, eax, ebx                      |   0C XX XX XX           						
----------------------------------------------------------------------------------------
div ecx, eax, ebx                       |   0D XX XX XX              					
----------------------------------------------------------------------------------------
addsub eax, eax, ebx, ecx               |   AB XX XX XX XX                				
----------------------------------------------------------------------------------------
subadd eax, eax, ebx, ecx               |   BA XX XX XX XX                
----------------------------------------------------------------------------------------
multidiv eax, eax, ebx, ecx             |   CD XX XX XX XX               
----------------------------------------------------------------------------------------
divmulti eax, eax, ebx, ecx             |   DC XX XX XX XX                				
----------------------------------------------------------------------------------------
addmulti eax, eax, ebx, ecx             |   AC XX XX XX XX											
----------------------------------------------------------------------------------------
adddiv eax, eax, ebx, ecx 	        |   AD XX XX XX XX											
----------------------------------------------------------------------------------------
multiadd eax, eax, ebx, ecx             |   CA XX XX XX XX											
----------------------------------------------------------------------------------------
submulti eax, eax, ebx, ecx            	|   BC XX XX XX XX											
----------------------------------------------------------------------------------------
subdiv eax, eax, ebx, ecx            	|   BD XX XX XX XX											
----------------------------------------------------------------------------------------
multisub eax, eax, ebx, ecx             |   CB XX XX XX XX											
----------------------------------------------------------------------------------------
divsub eax, eax, ebx, ecx             	|   DB XX XX XX XX													
----------------------------------------------------------------------------------------
cmp x,y                                 |   CC XX XX               
----------------------------------------------------------------------------------------
jmp                                     |   E0 XX                        
----------------------------------------------------------------------------------------
jl                                      |   E1 xx               
----------------------------------------------------------------------------------------
jle                                     |   E2 xx                
----------------------------------------------------------------------------------------
jg                                      |   E3 xx                 
----------------------------------------------------------------------------------------
jge                                     |   E4 xx                
----------------------------------------------------------------------------------------
jne                                     |   E5 xx                
----------------------------------------------------------------------------------------
je                                      |   E6 xx                
----------------------------------------------------------------------------------------
push					|   FA XX
----------------------------------------------------------------------------------------
pop					|   FF XX
----------------------------------------------------------------------------------------




